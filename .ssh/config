#Host *
#IdentityFile ~/.ssh/personal-key

Host *
    SetEnv TERM=xterm-256color

Host gitlab.com
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/personal-key
    IdentitiesOnly yes

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/personal-key
    IdentitiesOnly yes

Host git.vicugna.party
    HostName 192.168.0.122
    User git
    Port 2222
    IdentityFile ~/.ssh/personal-key
    IdentitiesOnly yes
