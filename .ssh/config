Host *
    IdentityAgent ~/.1password/agent.sock

Host gitlab.com
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/personal-key.pub
    IdentitiesOnly yes

Host pergit
    HostName github.com
    User git
    IdentityFile ~/.ssh/personal-key.pub
    IdentitiesOnly yes

Host git.vicugna.party
    HostName git.vicugna.party
    User git
    Port 222
    IdentityFile ~/.ssh/personal-key.pub
    IdentitiesOnly yes

Host 192.168.*
    IdentityFile ~/.ssh/personal-key.pub
