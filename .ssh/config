Host *
    IdentityFile ~/.ssh/personal-key

Host gitlab.com
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/personal-key
    IdentitiesOnly yes

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/personal-key
    IdentitiesOnly yes

Host git.vicugna.party
    HostName git.vicugna.party
    User git
    Port 222
    IdentityFile ~/.ssh/personal-key
    IdentitiesOnly yes
